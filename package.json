    "bcrypt": "^3.0.6",
    "cloudinary": "^1.14.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.5",
    "helmet": "^3.20.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.3.0-beta2",
